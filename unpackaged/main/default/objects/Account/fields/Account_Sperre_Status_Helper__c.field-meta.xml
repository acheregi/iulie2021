<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Account_Sperre_Status_Helper__c</fullName>
    <description>Account Sperre nicht wenn von SAP &quot;Inaktiv&quot; als Grund kommt, sonst bei allen anderen Ursachen</description>
    <externalId>false</externalId>
    <formula>OR(
LKM_KundenKontaktsperre__c, and(CL_Auftragssperre__c,not(ispickval(CL_AuftragssperreGrund__c ,&quot;Inaktiv&quot;)) ), CL_Fakturasperre__c, CL_ZurBearbeitungGesperrt__c)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Account Sperre Status Helper</label>
    <trackHistory>false</trackHistory>
    <type>Checkbox</type>
</CustomField>
